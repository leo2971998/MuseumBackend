{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "email-process.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/email-process",
      "dest": "email-process.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    {
      "path": "/email-process",
      "schedule": "*/1 * * * *"
    }
  ]
}
